<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>-250</x_min>
		<x_max>250</x_max>
		<y_min>-250</y_min>
		<y_max>250</y_max>
		<z_min>-10</z_min>
		<z_max>10</z_max>
		<dx>20</dx>
		<dy>20</dy>
		<dz>20</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">5000</max_time> <!-- 5 days * 24 h * 60 min = 14400 --> 
		<time_units>min</time_units>
		<space_units>micron</space_units>
	</overall>
	
	<parallel>
		<omp_num_threads>10</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>.</folder> <!-- use . for root --> 

		<full_data>
			<interval units="min">60</interval>
			<enable>true</enable>
		</full_data>
		
		<SVG>
			<interval units="min">60</interval>
			<enable>true</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<user_parameters>
	
		<seeding_method type="string" units="none" description="Method for seeding. Can be vertical,horizontal, random, or box (default)">box</seeding_method>

		<cell_default_necrotic_rate type="double" units="1/min" hidden="true" >0.0</cell_default_necrotic_rate>
		<cell_default_apoptotic_rate type="double" units="1/min" hidden="true">0.0</cell_default_apoptotic_rate>
		<cell_default_transition_rate type="double" units="1/min" hidden="true">0.0</cell_default_transition_rate>
		
		<cell_default_oxygen_uptake_rate type="double" units="1/min" hidden="true">0.0</cell_default_oxygen_uptake_rate>
		<cell_default_oxygen_secretion_rate type="double" units="1/min" hidden="true">0.0</cell_default_oxygen_secretion_rate>
		<cell_default_oxygen_saturation_density type="double" units="mmHg" hidden="true">0.0</cell_default_oxygen_saturation_density>
		
		<cell_default_glucose_uptake_rate type="double" units="1/min" hidden="true">0.0</cell_default_glucose_uptake_rate>
		<cell_default_glucose_secretion_rate type="double" units="1/min" hidden="true">0.0</cell_default_glucose_secretion_rate>
		<cell_default_glucose_saturation_density type="double" units="mol" hidden="true">10.0</cell_default_glucose_saturation_density>
		
		<cell_default_ECM_uptake_rate type="double" units="1/min" hidden="true">0.0</cell_default_ECM_uptake_rate>
		<cell_default_ECM_secretion_rate type="double" units="1/min" hidden="true">1.0</cell_default_ECM_secretion_rate>
		<cell_default_ECM_saturation_density type="double" units="mol" hidden="true">10.0</cell_default_ECM_saturation_density>		
		
		<cell_default_inital_energy type="double" units="a.u" hidden="false" description="Initialized energy level">7.01</cell_default_inital_energy>
		<cell_default_energy_creation_rate type="double" units="a.u/min" hidden="false" description="Energy creation rate for bacterial cells">0.01</cell_default_energy_creation_rate>
		<cell_default_energy_use_rate type="double" units="a.u/min" hidden="false" description="Energy usage rate for bacterial cells">0.02</cell_default_energy_use_rate>	
		<cell_default_cycle_energy_threshold type="double" units="a.u" hidden="false" description="Cycling energy threshold for bacterial cells">10.0 </cell_default_cycle_energy_threshold>
		<cell_default_death_energy_threshold type="double" units="a.u" hidden="false" description="Apoptosis energy threshold for bacterial cells">6</cell_default_death_energy_threshold>
		<cell_default_aplha type="double" units="dimensionless" hidden="false" description="Energy creation Rate modifier for aerobic cells">0.0</cell_default_aplha>			
		<cell_default_beta type="double" units="dimensionless" hidden="false" description="Energy creation Rate modifier for anaerobic cells">0.0</cell_default_beta>
		<cell_default_gamma type="double" units="dimensionless" hidden="false" description="Energy usage modulator for bacterial cells">0.5</cell_default_gamma>
		<cell_default_rho type="double" units="dimensionless" hidden="false" description="Energy degradation rate modifier for anaerobic cells">0.0</cell_default_rho>		
		<cell_default_phi type="double" units="dimensionless" hidden="false" description="Energy creation rate modulator for aerobic cells">1.0</cell_default_phi>
		<cell_default_chi type="double" units="dimensionless" hidden="false" description="Energy creation rate modulator for anaerobic cells">1.0</cell_default_chi>
		
		<cell_default_motility type="bool" units="none" hidden="true">false</cell_default_motility>
		
		<wound_cell_glucose_secretion_rate type="double" units="1/min" hidden="false" description="Glucose secretion rate for wound cells">10.0</wound_cell_glucose_secretion_rate>
		<wound_cell_ECM_secretion_rate type="double" units="1/min" hidden="false" description="Glucose secretion rate for wound cells">0.0</wound_cell_ECM_secretion_rate>
		
		<anaerobic_cell_alpha type="double" units="dimensionless" hidden="false" description="Energy Creation Rate modifier for aerobic cells">0.0</anaerobic_cell_alpha>			
		<anaerobic_cell_beta type="double" units="dimensionless" hidden="false" description="Energy Creation Rate modifier for anaerobic cells">1.0</anaerobic_cell_beta>
		<anaerobic_cell_gamma type="double" units="dimensionless" hidden="false" description="Energy usage modulator for bacterial cells">1.0</anaerobic_cell_gamma>
		<anaerobic_cell_rho type="double" units="dimensionless" hidden="false" description="Energy degradation rate modifier for anaerobic cells">0.1</anaerobic_cell_rho>		
		<anaerobic_cell_phi type="double" units="dimensionless" hidden="false" description="Energy creation rate modulator for aerobic cells">1.0</anaerobic_cell_phi>
		<anaerobic_cell_chi type="double" units="dimensionless" hidden="false" description="Energy creation rate modulator for anaerobic cells">2.0</anaerobic_cell_chi>
		
		<anaerobic_ECM_secretion_rate type="double" units="1/min" hidden="false" description="ECM creation rate for anaerobic cells">1.0</anaerobic_ECM_secretion_rate>
		<anaerobic_glucose_uptake_rate type="double" units="1/min" hidden="false" description="Glucose uptake rate for anaerobic cells">1.0</anaerobic_glucose_uptake_rate>		
		
		<aerobic_cell_alpha type="double" units="dimensionless" hidden="false" description="Energy Creation Rate modifier for aerobic cells">1.0</aerobic_cell_alpha>			
		<aerobic_cell_beta type="double" units="dimensionless" hidden="false" description="Energy Creation Rate modifier for anaerobic cells">0.0</aerobic_cell_beta>
		<aerobic_cell_gamma type="double" units="dimensionless" hidden="false" description="Energy usage modulator for bacterial cells">1.0</aerobic_cell_gamma>
		<aerobic_cell_rho type="double" units="dimensionless" hidden="false" description="Energy degradation rate modifier for anaerobic cells">0.0</aerobic_cell_rho>		
		<aerobic_cell_phi type="double" units="dimensionless" hidden="false" description="Energy creation rate modulator for aerobic cells">1.0</aerobic_cell_phi>
		<aerobic_cell_chi type="double" units="dimensionless" hidden="false" description="Energy creation rate modulator for anaerobic cells">1.0</aerobic_cell_chi>	
		
		<aerobic_ECM_secretion_rate type="double" units="1/min" hidden="false" description="ECM creation rate for aerobic cells">1.0</aerobic_ECM_secretion_rate>
		<aerobic_glucose_uptake_rate type="double" units="1/min" hidden="false" description="Glucose uptake rate for aerobic cells">1.0</aerobic_glucose_uptake_rate>		
		<aerobic_oxygen_uptake_rate type="double" units="1/min" hidden="false" description="Oxygen uptake rate for aerobic cells">1000.0</aerobic_oxygen_uptake_rate>		
		

		
		<apoptosis_rate type="double" units="1/min" hidden="false" description="Apoptosis rate for bacterial cells">0.01</apoptosis_rate>		
		<proliferation_rate type="double" units="1/min" hidden="false" description="Proliferation rate for bacterial cells">0.001</proliferation_rate>
		
		<ECM_diffusion_coeff type="double" units="um2/min" hidden="false">0.01</ECM_diffusion_coeff>		
		<ECM_decay_constant type="double" units="1/min" hidden="false">1</ECM_decay_constant>

		<glucose_diffusion_coeff type="double" units="um2/min" hidden="false">1.6e3</glucose_diffusion_coeff>		
		<glucose_decay_constant type="double" units="1/min" hidden="false">0.05</glucose_decay_constant>

		
	</user_parameters>
	
</PhysiCell_settings>
